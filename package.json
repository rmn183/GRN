{
  "name": "InoappsPogrnReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "android:debug-dev": "cd android && ENVFILE=.env.dev ./gradlew installCamdenDevDebug",
    "android:debug-test": "cd android && ENVFILE=.env.test ./gradlew installCamdenTestDebug",
    "android:debug-prod": "cd android && ENVFILE=.env.prod ./gradlew installCamdenProdDebug",
    "android:build-dev": "cd android && ENVFILE=.env.dev ./gradlew assembleCamdenDevRelease",
    "android:build-test": "cd android && ENVFILE=.env.test ./gradlew assembleCamdenTestRelease",
    "android:build-prod": "cd android && ENVFILE=.env.prod ./gradlew assembleCamdenProdRelease",
    "ios:debug-dev": "react-native run-ios --scheme=\"InoappsPogrnReactNativeCamdenDev\"",
    "ios:debug-test": "react-native run-ios --scheme=\"InoappsPogrnReactNativeCamdenTest\"",
    "ios:debug-prod": "react-native run-ios --scheme=\"InoappsPogrnReactNativeCamdenProd\""
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "apisauce": "^2.1.1",
    "capitalize": "^2.0.3",
    "format-json": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "js-base64": "^3.6.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "native-base": "^3.0.6",
    "prop-types": "^15.7.2",
    "querystringify": "2.2.0",
    "ramda": "^0.27.1",
    "react": "17.0.2",
    "react-moment": "^1.1.1",
    "react-native": "^0.64.2",
    "react-native-animatable": "^1.3.3",
    "react-native-check-box": "^2.1.7",
    "react-native-config": "^1.4.3",
    "react-native-device-info": "^8.1.4",
    "react-native-expandable-section-flatlist": "^1.2.2",
    "react-native-flash-message": "^0.1.23",
    "react-native-i18n": "^2.0.15",
    "react-native-image-picker": "^4.0.6",
    "react-native-loading-spinner-overlay": "^2.0.0",
    "react-native-modal-datetime-picker": "^10.2.0",
    "react-native-modest-checkbox": "^3.3.0",
    "react-native-schemes-manager": "^2.0.0",
    "react-native-switch": "^2.0.0",
    "react-native-vector-icons": "^8.1.0",
    "react-navigation": "^4.4.4",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-redux": "^7.2.4",
    "realm": "^10.6.0",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reduxsauce": "1.2.1",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@storybook/addon-storyshots": "^6.3.5",
    "@storybook/addons": "^6.3.5",
    "@storybook/channels": "^6.3.5",
    "@storybook/react-native": "^5.3.25",
    "babel-eslint": "^7.1.1",
    "babel-jest": "27.0.6",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "husky": "^7.0.1",
    "ignite-animatable": "^1.0.2",
    "ignite-dev-screens": "^2.4.5",
    "ignite-i18n": "^1.2.0",
    "ignite-ir-boilerplate-andross": "^2.3.3",
    "ignite-redux-persist": "^2.0.0",
    "ignite-standard": "^1.0.0",
    "ignite-vector-icons": "^1.1.1",
    "jest": "27.0.6",
    "mockery": "^2.1.0",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "snazzy": "^9.0.0",
    "standard": "^16.0.3"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/Tests/Setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
